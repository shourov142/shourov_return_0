1.5:   (a) #include<stdio.h>
main()
{
float F,C;
C=200;
F=(((9*C)/5)+32);
printf("Celsius = %5.2f to Fahrenheit = %5.2f\n",C,F);
}


(b) #include<stdio.h>
main()
{
float F,C;
F=300;
C=((F-32)*5)/9;
printf("Fahrenheit =%5.2f to Celsius = %5.2f\n",F,C);
}



1.6: #include<stdio.h>
main()
{
float PIE,Rad,Area;
PIE=3.1416;
Rad=4;
Area=PIE*Rad*Rad;
printf("Area of a circle is %f",Area);
}





