3.4: #include<stdio.h>
main()
{
    float length,width,Area,Perimeter;
    printf("Enter The length:");
    scanf("%f", &length);
    printf("Enter The width:");
    scanf("%f",&width);
    Area=length*width;
    Perimeter=2*(length+width);
    printf("The Area of the rectangle:%.2f\n",Area);
    printf("The Perimeter of the rectangle:%.2f",Perimeter); }
3.5: #include<stdio.h>
main()
{
    int x,a,b,c;
        printf("Enter the Integer Number:\n");
        printf("x=");
        scanf("%d",&x);
        a=x%1000;
        b=x%100;
        c=x%10;
        printf("%d\n",x);
