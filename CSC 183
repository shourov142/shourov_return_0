3.13: #include<stdio.h>
main()
{
  int x,y,z;
  printf("Enter Three Numbers:\n");
  scanf("%d %d %d",&x,&y,&z);
 ((x>y)&&(x>z))?printf("Largest is x : %d",x):
 ((y>x)&&(y>z))?printf("Largest is y : %d",y):
  printf("Largest is z : %d",z);
}


3.14: #include<stdio.h>
main()
{
int m,n,x;

printf("Enter Two Numbers:\n");
scanf("%d %d",&m,&n);
x=m%n;
(x==0)?printf("m is multiple of n\n"):printf("m is not multiple of n\n");
}





3.15: #include<stdio.h>
 main()
{
    int a,b,c;
    float sum,avv;
    printf("Enter the value:\n");
    scanf("%d%d%d",&a,&b,&c);
    sum=a+b+c;
    avv=(a+b+c)/3;
    printf("Sum is : %f\n",sum);
    printf("Average is : %f\n",avv);

   ((a>b)&&(a>c))?printf("Largest is : %d\n",a):
   ((b>a)&&(b>c))?printf("Largest is : %d\n",b):
    printf("Largest is : %d\n",c);

   ((a<b)&&(a<c))?printf("Smallest is : %d",a):
   ((b<a)&&(b<c))?printf("Smallest is : %d",b):
    printf("Smallest is : %d",c);
}

3.16: #include<stdio.h>
main()
{
float cc, bill;
printf("Enter Numbers of Call of Customer:");
scanf("%f",&cc);
(cc<100)?(bill=250):(bill=250+cc*1.25);
printf("Mobile Bill of Customer : %.2f",bill);
}
















3.17: #include<stdio.h>
#include<math.h>
 main()
 {
     int a=0,b=15,c=30,d=45;
     double w,x,y,z,W,X,Y,Z;
     w=sin(a);
     x=sin(b);
     y=sin(c);
     z=sin(d);
     W=cos(a);
     X=cos(b);
     Y=cos(c);
     Z=cos(d);
     printf("x(degrees)            sin(x)                   cos(x)\n");
     printf("   0                  %lf                 %lf\n",w,W);
     printf("   15                 %lf                 %lf\n",x,X);
     printf("   30                 %lf                 %lf\n",y,Y);
     printf("   45                 %lf                 %lf\n",z,Z);
 }




3.18: #include<stdio.h>
#include<math.h>
main()
 {
     int a=0,b=10,c=20,d=30;
     double w,x,y,z,W,X,Y,Z;
     w=sqrt(a);
     x=sqrt(b);
     y=sqrt(c);
     z=sqrt(d);
     W=a*a;
     X=b*b;
     Y=c*c;
     Z=d*d;

     printf("\tNumber              Square-root                  Square\n");
     printf("\t   0                  %.2lf                 %.2lf\n",w,W);
     printf("\t   10                 %.2lf                 %.2lf\n",x,X);
     printf("\t   20                 %.2lf                 %.2lf\n",y,Y);
     printf("\t   30                 %.2lf                 %.2lf\n",z,Z);

 }



