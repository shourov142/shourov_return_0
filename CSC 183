 scanf("%f",& RealNo);

 SmallNo=RealNo;
 LargeNo=RealNo;

 printf("Smallest integer              The given                  Largest integer \n");
 printf("not less then                 number                     not grether then\n");
 printf("the number                                               the number\n");
 printf("   %d                          %.2f                           %d\n",SmallNo,RealNo,LargeNo);

 }
3.8:main()
{
int u,t,a;
float Dis;
printf("Enter the value of u,a and t\n");
scanf("%d %d %d",&u,&a,&t);
Dis=(u*t)+(a*(t*t))/2;
printf("The distance is : %.2f \n",Dis);
}
3.9: #include<stdio.h>
#include<math.h>
main()
{
float Dr,Sc,Hc;
float TBO,EOQ;
printf("Enter Demand Rate :");
scanf("%f\n",&Dr);
printf("Enter Setup Cost :");
scanf("%f\n",&Sc);
printf("Enter Holding Cost :");
scanf("%f\n",&Hc);
EOQ=sqrt((2*Dr*Sc)/Hc);
TBO=sqrt((2*Sc)/(Dr*Hc));
printf("The Economic Order Quantity is : %f\n",EOQ);
printf("The time Between Order is : %f",TBO);
}

3.10: #include<stdio.h>
#include<math.h>
int main()
{
  double L,R,C;
  double Freq,Temp1,Temp2;

  printf("Enter Inductance:");
  scanf("%lf",&L);
  printf("Enter Resistance:");
  scanf("%lf",&R);
  printf("Enter Capacitannce:");
  scanf("%lf",&C);
  Temp1= (1/(L*C));
  Temp2= (R*R)/(4*C*C);
  Freq= sqrt(Temp1-Temp2);
  printf("The Frequency is : %lf",Freq);
}



3.11: #include<stdio.h>
 main()
{
int Num,Sum,Sum1,Sum2,Sum3,Sum4;
printf("Enter a Four Digits Number\n",&Num);
scanf("%d",&Num);
Sum1=Num%10;
Num=Num/10;
Sum2=Num%10;
Num=Num/10;
Sum3=Num%10;
Num=Num/10;
Sum4=Num%10;
Num=Num/10;
Sum=Sum1+Sum2+Sum3+Sum4;
printf("\nSum of Digits are : %d\n",Sum);
}
