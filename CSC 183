3.1: #include<stdio.h>
main()
{
    int x,y,z,a;
    printf("Enter the 3 values:\n");
    scanf("%d%d%d",&x,&y,&z);
    a=x;
    x=y;
    y=z;
    z=a;
    printf("x=%d,y=%d,z=%d",x,y,z);
}
3.2: #include<stdio.h>
main()
{
    int x,y,z;
    printf("Enter the floating point number:\n");
    printf("x=");
    scanf("%d",&x);
    y=x;
    z=y%10;
    printf("The Right-most digit of the integral part of the number %.3f is %d",x,z);
}

