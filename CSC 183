        printf("%d\n",c);
}










3.6: #include<stdio.h>
main()
{
    float Dep,Pur_price,Sal_value,Year_ser;
    printf("Enter The Depreciation:");
    scanf("%f",&Dep);
    printf("Enter The Purchase Price:");
    scanf("%f",&Pur_price);
    printf("Enter The Year of Service:");
    scanf("%f",&Year_ser);
    Sal_value= Pur_price-(Dep*Year_ser);
    printf("Salvage Value:%.2f",Sal_value);
}
