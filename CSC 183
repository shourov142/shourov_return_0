1.11:#include<stdio.h>
#include<math.h>
main()
{
int a,b,c;
float S,A;
a=b=c=S=0;
a=20;
b=30;
c=40;
S=(a+b+c)/2;
A= sqrt(S*(S-a)*(S-b)*(S-c));
printf("Area of a triangle is= %.2f",A);
}











1.12:#include<stdio.h>
#include<math.h>
main()
{
int x,y;
float Sum,Diff,Prod,Div;
x=6;
y=5;
Sum=x+y;
Diff=x-y;
Prod=x*y;
Div=x/y;
printf("x=%.1d y=%.1d\n",x,y);
printf("Sum=%.1f Difference=%.1f\n",Sum,Diff);
printf("Product= %5.1f Dividion= %5.1f",Prod,Div);
}








1.13: #include<stdio.h>
#include<math.h>
main()
{
int x1,x2,y1,y2;
float D;
D=0;
x1=20;
x2=30;
y1=40;
y2=50;
D=sqrt((x2-x1)*(x2-x1)+ (y2-y1)* (y2-y1));
printf("Distance between to points is= %.4f",D);
}











1.15: #include<stdio.h>
#include<math.h>
#define PIE 3.14
main()
{
int x1,y1,x2,y2;
float Die,Rad,Area;
x1=2;
y1=2;
x2=5;
y2=6;
Die=sqrt((x2-x1)*(x2-x1)+ (y2-y1)* (y2-y1));
Rad=Die/2;
Area=PIE*Rad*Rad;
printf("Area is= %.3f",Area);
}
